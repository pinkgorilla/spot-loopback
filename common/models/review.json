{
  "name": "Review",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "rating": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "comment": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "reviewer": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId"
    },
    "location": {
      "type": "belongsTo",
      "model": "Location",
      "foreignKey": "locationId"
    }
  },
  "acls": [],
  "methods": {}
}
